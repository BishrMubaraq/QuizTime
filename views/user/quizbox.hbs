{{! Quiz Box }}
<section>
  <div class="container">
    <h3 class="Title">{{category.CategoryName}}</h3>
    <div class="content-box">
      {{#each questionBank}}
      <div class="quiz_question">
        <span><h5 class="question">{{this.Question}}</h5></span>
        <div class="option-list">
          <div class="options" onclick="optionSelected(this)">
            <span>A. {{this.A}}</span>
          </div>
          <div class="options" onclick="optionSelected(this)">
            <span>B. {{this.B}}</span>
          </div>
          <div class="options" onclick="optionSelected(this)">
            <span>C. {{this.C}}</span>
          </div>
          <div class="options" onclick="optionSelected(this)">
            <span>D. {{this.D}}</span>
          </div>
        </div>
      </div>
    </div>
    <div>
      <button type="button" class="btn">Back</button>
      <button type="button" class="btn">Next</button>
    </div>
  </div>
</section>

{{! Description }}
<article>
  <div class="container">
    <div class="description">
      <p class="descriptionText">
        {{this.Description}}
      </p>
    </div>
  </div>
</article>
{{/each}}